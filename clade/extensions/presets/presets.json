{
    "base": {
        "extends": [],
        "PidGraph.filter_cmds_by_pid": true,
        "Common.save_unparsed_cmds": true,
        "CC.filter_deps": true,
        "CC.store_deps": true,
        "log_level": "INFO"
    },
    "print_graphs": {
        "extends": [],
        "CmdGraph.as_picture": true,
        "PidGraph.as_picture": true,
        "SrcGraph.as_picture": true
    },
    "linux_kernel": {
        "extends": ["base"],
        "CmdGraph.requires": [
            "CC",
            "LD",
            "MV",
            "AR",
            "Objcopy"
        ],
        "Common.filter": [
            ".*?\\.tmp$"
        ],
        "Common.filter_in": [
            "-",
            "/dev/null",
            "scripts/(?!mod/empty\\.c)",
            "kernel/.*?bounds.*?",
            "arch/x86/tools/relocs",
            "arch/x86/kernel/asm-offsets.c",
            ".*\\.mod\\.c",
            ".*?\\.tmp\\w+\\.s",
            ".*\\.S"
        ],
        "Common.filter_out": [
            "/dev/null",
            "scripts/(?!mod/.*empty\\.o)",
            ".*?\\.cmd$",
            "vmlinux"
        ]
    },
    "busybox_linux": {
        "extends": ["base", "print_graphs"],
        "Common.filter": [],
        "Common.filter_in": [
            ".*?\\.tmp$",
            "tmp\\.\\w+\\.c$",
            "^tmp.*?\\.c$",
            "/tmp/\\w+.o",
            "-",
            "/dev/null",
            ".*?built-in\\.o$"
        ],
        "Common.filter_out": [
            "/dev/null",
            ".*?\\.cmd$",
            "tmp\\.\\w+"
        ]
    },
    "apache_linux": {
        "extends": ["base", "print_graphs"],
        "Common.filter": [
            "/tmp/.*"
        ],
        "Common.filter_in": [
            ".*?\\.tmp$",
            "tmp\\.\\w+\\.c$",
            "^tmp.*?\\.c$",
            "-",
            "/dev/null",
            ".*?built-in\\.o$"
        ],
        "Common.filter_out": [
            "/dev/null",
            ".*?\\.cmd$",
            "tmp\\.\\w+"
        ]
    }
}
